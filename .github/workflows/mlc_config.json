{
  "ignorePatterns": [
    {
      "pattern": "^https://youtube.com/example"
    },
    {
      "pattern": "^https://youtube.com/@AutumHorizon"
    },
    {
      "pattern": "^https://linkedin.com/in/AutumHorizon"
    },
    {
      "pattern": "^\\[.*\\]\\(#.*\\)$"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept": "application/vnd.github.v3+json"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackHttpStatus": [
    400,
    401,
    403,
    404,
    500,
    502,
    503
  ]
}